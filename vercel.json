{
  "routes": [
    { "src": "/.*", "dest": "/index.html" },
    {
      "src": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://www.bedrijfsinrichtingnederland.nl"
        }
      ]
    }
  ]
}
